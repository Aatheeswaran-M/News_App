{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/news/(.*)",
      "destination": "/news/index.html"
    },
    {
      "source": "/news",
      "destination": "/news/index.html"
    }
  ],
  "headers": [
    {
      "source": "/news/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}